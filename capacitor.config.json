{
  "appId": "com.yourcompany.ckdapp",
  "appName": "CKD Prediction",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "Camera": {
      "photos": true
    },
    "Geolocation": {
      "precision": "high"
    }
  },
  "server": {
    "url": "http://localhost",
    "cleartext": true
  },
  "ios": {
    "minVersion": "13.0",
    "scheme": "CKDApp"
  },
  "android": {
    "minVersion": "22",
    "allowMixedContent": true
  }
}
