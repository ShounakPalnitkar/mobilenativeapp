{
  "name": "ckd-prediction-tool",
  "version": "1.0.0",
  "description": "Chronic Kidney Disease Prediction Tool",
  "main": "index.js",
  "scripts": {
    "start": "serve -s dist",
    "build": "mkdir -p dist && cp src/index.html dist/ && cp -r src/assets dist/",
